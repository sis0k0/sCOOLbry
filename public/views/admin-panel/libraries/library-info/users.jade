.tab-pane.fade#library-users
  h2 #{__("Library users")}
  table.table.table-striped.table-bordered.table-wrapped
    tr
      th #{__("Username")}
      th #{__("Books given")}
      th #{__("Books to return")}
      th

    tr(ng-repeat='user in libraryUsers')
      td.col-md-4
        strong {{ user.username }}
      td {{user.given}}
      td {{user.toReturn}}
      td
        a(ng-href='/admin/user/{{user.userID}}') #{__("Link")}
.container
  .well
    h1.text-center Title: {{ bookInfo.title }}
    table.table.table-striped.table-bordered.table-wrapped 
      tr(ng-repeat='(key, value) in bookInfo')
        td.col-md-4
          strong {{ key }}
        td {{ value }}

    table
      tr
        td
          h2 Readers
      tr
        td
          h2 Libraries Owning the Book
      tr
        td
          h2 Book Reviews
